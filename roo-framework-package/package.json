{
  "name": "@sdbingham/roo-framework",
  "version": "4.5.4",
  "description": "Structured, Transparent, and Well-Documented AI Team Framework with Docker Integration",
  "main": "index.js",
  "bin": {
    "roo-framework": "cli.js"
  },
  "scripts": {
    "postinstall": "node scripts/post-install.js",
    "setup": "node scripts/setup.js",
    "cli": "node cli.js",
    "boomerang": "node cli.js boomerang",
    "mode": "node cli.js mode",
    "diagnose": "node scripts/diagnose-root-issue.js",
    "docker-health": "node scripts/docker-health-check.js",
    "docker:health": "node scripts/docker-health-check.js",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:restart": "docker-compose down && docker-compose up -d",
    "generate-docs": "node scripts/generate-docker-docs.js",
    "test": "echo \"No tests available\"",
    "langchain-example": "node examples/langchain-memory-example.js",
    "test-langchain": "node scripts/test-langchain-integration.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdbingham/The-Ultimate-Roo-Code-Hack-Building-a-Structured-Transparent-and-Well-Documented-AI-Team.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "roo",
    "ai",
    "framework",
    "multi-agent",
    "llm",
    "agent",
    "orchestration",
    "boomerang",
    "langchain"
  ],
  "author": "Sean Bingham",
  "contributors": [
